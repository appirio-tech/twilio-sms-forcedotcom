<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Stores information related to Twilio. Phone numbers, Account Sid, Auth Token, etc.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Account_Sid__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Twilio Account SID.</inlineHelpText>
        <label>Account Sid</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Auth_Token__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Twilio Auth Token.</inlineHelpText>
        <label>Auth Token</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Primary_Phone__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The Twilio phone number that will be used to send and receive SMS Messages.</inlineHelpText>
        <label>Primary Phone #</label>
        <required>false</required>
        <type>Phone</type>
    </fields>
    <fields>
        <fullName>Twilio_Status_Listen_Page__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>URL of sites page for smsStatusListen Visualforce page.</inlineHelpText>
        <label>Twilio Status Listen Page</label>
        <required>false</required>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>Unsubscribe_Keywords__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Comma separated list of words that if user responds with will uncheck the SMS Enalbed field on Contact.</inlineHelpText>
        <label>Unsubscribe Keywords</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <label>Twilio Settings</label>
</CustomObject>
